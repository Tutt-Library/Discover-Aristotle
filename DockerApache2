# Dockerfile for DigitalCC Apache2 reverse-proxy
FROM httpd:2.4
MAINTAINER Jeremy Nelson <jermnelson@gmail.com>

RUN apt-get update && \
    apt-get install -y libapache2-mod-wsgi
# Dockerfile for DigitalCC Apache2 web server
FROM httpd:2.4
MAINTAINER Jeremy Nelson <jermnelson@gmail.com>

COPY ./vhosts.conf /usr/local/apache2/conf/vhosts.conf
